@article{ParkerVanAlstyne2005,
  abstract = {How can firms profitably give away free products? This paper provides a novel answer and articulates trade-offs in a space of information product design. We introduce a formal model of two-sided network externalities based in textbook economics-a mix of Katz and Shapiro network effects, price discrimination, and product differentiation. Externality-based complements, however, exploit a different mechanism than either tying or lock-in even as they help to explain many recent strategies such as those of firms selling operating systems, Internet browsers, games, music, and video. The model presented here argues for three simple but useful results. First, even in the absence of competition, a firm can rationally invest in a product it intends to give away into perpetuity. Second, we identify distinct markets for content providers and end consumers and show that either can be a candiyear for a free good. Third, product coupling across markets can increase consumer welfare even as it increases firm profits. The model also generates testable hypotheses on the size and direction of network effects while offering insights to regulators seeking to apply antitrust law to network markets.},
  author = {Parker, Geoffrey G. and Van Alstyne, Marshall W.},
  publisher = {INFORMS},
  year = {2005-10},
  doi = {10.1287/mnsc.1050.0400},
  issn = {0025-1909},
  journal = {Management Science},
  number = {10},
  pages = {1494--1504},
  shorttitle = {Two-sided network effects},
  title = {Two-Sided Network Effects: A Theory of Information Product Design},
  volume = {51},
}

@techreport{ParkerVanAlstyne2013,
  abstract = {Consider that a firm in charge of a business platform is a firm in charge of a microeconomy. To achieve the highest growth rate, how open should that economy be? To encourage third-party developers, how long should their intellectual property interests last? We no\_address these questions through a sequential innovation model that balances two sets of tradeoffs. (i) Closing the platform increases the sponsor's ability to charge for access, while opening the platform increases developer ability to build upon it. (ii) The longer third-party developers retain property rights to their innovations, the higher the royalties they and the sponsor earn, but the sooner those developers rights expire, the sooner their innovations become a public good upon which other ecosystem developers can build. Our model allows us to characterize the optimal levels of openness and of IP duration in a platform ecosystem. We also model the relative profit potential of a sponsoring firm's decision to vertically integrate into downstream production as a closed hierarchy or to organize itself as a platform for open innovation. We use standard tools of Cobb-Douglas production and two-sided networks to derive our results. These findings can inform innovation strategy, choice of organizational form, intellectual property law, and management of competition.},
  author = {Parker, Geoffrey and Van Alstyne, Marshall W.},
  institution = {Massachusetts Institute of Technology},
  year = {2013-12},
  doi = {10.2139/ssrn.1079712},
  number = {4684-08},
  title = {Innovation, Openness, and Platform Control},
  type = {MIT Sloan School Working Paper},
}

@article{Patterson2004,
  abstract = {There must be 4,000 programmers typing away in their basements trying to build the next "world's most scalable" search engine. It has been done only a few times. It has never been done by a big group; always one to four people did the core work, and the big team came on to build the elaborations and the production infrastructure. Why is it so hard? We are going to delve a bit into the various issues to consider when writing a search engine. This article is aimed at those individuals or small groups that are considering this endeavor for their Web site or intranet. It is fun, but a word of caution: not only is it difficult, but you need two commodities in short supply: time and patience.},
