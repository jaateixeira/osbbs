@inproceedings{SapsedPare2013,
  abstract = {The cultural and creative industries have been identified as important stimulants of economic development and regeneration in the advanced industrialised countries, but what are the opportunities they offer for developing countries? This paper reviews recent research and thinking and suggests four themes and propositions relating to creative industrialisation in the South. Firstly, it observes social network research, which shows the surprising reality of creative and business dynamics occurring in the core and periphery of cultural networks. These have promising implications for the developing world. Second, it discusses the symbolic economy and how the appetite for new ideas, signs and motifs will inevitably lead the creative industries to search in developing cultures. Third, it considers the notion of regional development through creative cities, regions and festivals. Fourth, it turns attention to the implications of digital technologies and the Internet for developing countries, discussing the entry barriers and disintermediation debates as well as the long tail phenomenon. The paper aims to stimulate focused and critical discussion on the viability of creative industrialisation pathways to development.},
  author = {Sapsed, Jonathan and Paré, Daniel},
  organization = {International Association for Management of Technology},
  booktitle = {``Science, Technology and Innovation in the Emerging Markets Economy'', Proceedings of the 22nd International Conference on Management of Technology},
