@inproceedings{PulkkanenSeppanen2012,
  abstract = {This study analyzed the business models of freemium companies in order to identify what are the crucial factors in applying the freemium business model. Freemium refers to business model where a certain product for customer if offered free of charge while profiting from charging a premium for more advanced version of the product. The analyses are based on five successful, secondary data cases: Spotify, Pandora, Wild Apricot, 37signals, and Evernote. The findings emphasize clear separation of free and premium versions that must be based on user needs. Also, the role of free version must be acknowledged to support other businesses in the firm. From the marketing perspective, the management has to be able to identify when to move from focus to optimization: in the first phase a firm has to focus audaciously and aggressively on free version customers, and after the chasm focus has to be shifted to profitability optimization.},
  author = {Pulkkanen, Aleksi and Seppänen, Marko},
  booktitle = {Proceedings of the XXIII ISPIM Conference --- Action for Innovation: Innovating from Experience},
  year = {2012-06},
  isbn = {978-952-265-242-3},
