@article{TidstromRajala2016,
	title        = {Coopetition strategy as interrelated praxis and practices on multiple levels},
	author       = {Tidstr\"{o}m, Annika and Rajala, Anni},
	journal      = {Industrial Marketing Management},
	volume       = 58,
	pages        = {35--44},
	doi          = {10.1016/j.indmarman.2016.05.013},
	issn         = {0019-8501},
	url          = {https://www.sciencedirect.com/science/article/pii/S0019850116300876},
	abstract     = {Existing research often considers coopetition as something occurring between organizations on a meso level, that is, a relational or company level. This study explores coopetition strategy in the form of activities from a multilevel perspective. The focus is on coopetition praxis and practices and how these are interrelated on the micro, meso and macro levels. In order to improve our understanding of coopetition activities, we use the strategy-as-practice approach and integrate it with coopetition research. The empirical part of this study is based on a single qualitative case study of a coopetitive relationship between a large multinational company and its supplier. The findings show how praxis on the micro level influences, and is influenced by, practices on the meso and macro levels. Coopetition strategy is shaped over time through the relationship between praxis and practices on different levels. Our study contributes to coopetition research by focusing on coopetition strategy as something that is implemented on multiple levels, including the individual (micro) and network (macro) levels alongside the meso level.},
	year         = 2016
}


