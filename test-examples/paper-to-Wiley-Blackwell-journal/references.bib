  title = {The dynamism of the two-sided application store market},
  volume = {6},
}

@inproceedings{Jayaraman2011,
  abstract = {Visualizing software as ecosystems has been an emergent phenomenon. This has been fueled by the recent success of mobile application ecosystems of Apple and Google. The objective of this paper is to examine the field of software ecosystems (SECO) and provide a critical review of the existing research by identifying domains and peripheries, highlighting architectural challenges by examining design and control mechanisms in a SECO context and comparing SECO with other popular paradigms. This paper also aims to postulate the future role of software ecosystems in the broader context of information systems research by examining gaps in the existing research and proposing future directions.},
  author = {Jayaraman, Karthik},
  booktitle = {Proceedings of the 11th International Conference on Electronic Business},
  year = {2011-11},
  series = {ICEB 2011},
  title = {Making Sense of Software Ecosystems: A Critical Review},
}

@article{JeonRochet2010,
  abstract = {More and more academic journals are adopting an open access policy by which articles are accessible free of charge, while publication costs are recovered through author fees. We study the consequences of this open access policy on the quality standard of an electronic academic journal. If the journal's objective were to maximize social welfare, open access would be optimal. However, we show that if the journal has a different objective (such as maximizing readers' utility, the impact of the journal, or its profit), open access tends to induce it to choose a quality standard below the socially efficient level. (JEL L11, L82)},
  author = {Jeon, Doh-Shin and Rochet, Jean Charles},
